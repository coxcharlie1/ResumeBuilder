{
    "configurations": [
        {
            "type": "java",
            "name": "Spring Boot-ResumeBuilderApplication<resume-builder>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.charliecox.resumebuilder.ResumeBuilderApplication",
            "projectName": "resume-builder",
            "args": "",
            "envFile": "${workspaceFolder}/.env",
            "vmArgs": " -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=61199 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dspring.jmx.enabled=true -Djava.rmi.server.hostname=localhost -Dspring.application.admin.enabled=true -Dspring.boot.project.name=resume-builder"
        }
    ]
}